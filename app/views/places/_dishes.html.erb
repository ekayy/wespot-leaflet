<ul id="dishesContainer">
    <% @dishes.each do |dish| %>

        <li class="dishItem">
            <%= link_to image_tag(dish.image_url(:medium)), "#{dish.image}", class: 'showImage' %>
            <%= dish.name %>
            <div class="dishText"><%= dish.description %></div>
            <%= link_to "Comments", '#', class: 'fr' %>
        </li>

      <% end %>
  </ul>
  <% 5.times do %>
  <li class="dishItem">
    <div class="showImage" style="background:#888;width:100px;height:100px;"></div>
    <div class="dishText">Dish description here.</div>
    <%= link_to "Comments", '#', class: 'fr' %>
  </li>
  <% end %>

