
<h3>Top Dishes</h3>

  <ul id="dishesContainer">
    <%# @dishes.each do |dish| %>
    <% 5.times do %>
        <li class="dishItem">
            <%# link_to image_tag(dish.image_url(:medium)), "#{dish.image}", class: 'gram' %>
            <div class="showImage" style="background:#888;width:75px;height:75px;"></div>
          <%# dish.name %>
            <div class="dishText">Lorem ipsum dolor sit amet, consectetur adipiscing elit. In in massa sem, tempor sodales nunc.  </div>
            <%= link_to "Comments", '#', class: 'fr' %>
        </li>
      <% end %>
  </ul>

