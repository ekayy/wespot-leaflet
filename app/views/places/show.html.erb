      $(".coverphoto2").backstretch('<%= raw @place.coverphoto_url(:medium).to_s %>');
    });
  </script>



<div class="container">

    <div class="coverphoto2"></div>
    <h1 style="font-size:24px;"><%= @place.business_name %></h1>

    <div class="span8">


        <div class="col1">
          <%= render 'twitter' %>
          <%= render 'instagram' %>
          <%= render 'highlights' %>
        </div>
    </div>

    <div class="span4">
      <div class="col2">

        <%= render 'staticmap' %>
        <%= render 'dishes' %>

      </div>
    </div>

