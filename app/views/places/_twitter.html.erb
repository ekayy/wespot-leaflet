<div id="bizContainer">
<h3>Recent News</h3>

<% @twitter.each do |twitter| %>
      <ul>
        <span class="avatar"><%= image_tag twitter.user.profile_image_url %>
        <strong><%= twitter.user.name %></strong></span>
      </br>
        <%= twitter.text %>
        <%= twitter.created_at %>
      </ul>
      <% end %>
</div>