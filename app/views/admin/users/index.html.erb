<h1>Listing users</h1>

 <table>
    <tr>
      <th>Username</th>
      <th>Email</th>
    </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'Edit User', edit_admin_user_path(user) %></td>
      <td><%= link_to 'Delete User', user, method: :delete,
                                  data: { confirm: "You sure?" } %></td>
    </tr>

  <% end %>
    
  </table>

  <br />

  <%= link_to 'New User', new_admin_user_path %>
